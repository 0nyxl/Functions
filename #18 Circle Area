#include <iostream>
#include<string>
using namespace std;

double Circle Area(double A)
{
    
    const float PI = 3.14;
    return PI * (A * A);
   }

int main()
{
    double A;
    cout << "Please Enter Circle Area: ";
    cin >> A;
    cout<<endl<<"Your Circle Area Is: " << Circle Area(A) <<endl<<endl;
    
    
    return 0;
}
